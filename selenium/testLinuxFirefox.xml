<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test in linux for firefox browser">
    <test name="testLinuxFirefox">
        <parameter name="browser" value="firefox"/>
        <parameter name="browserVersion" value="null"/>
        <parameter name="remote" value="https://localhost:4444/wd/hub"/>
        <parameter name="headless" value="true"/>
        <parameter name="timeout" value="8000"/>
        <parameter name="baseUrl" value="https://localhost:8080/"/>
        <classes>
            <class name="uitest.mainPage.MainPageTest"/>
            <class name="uitest.contacts.AddContactsPageTest"/>
            <class name="uitest.organizations.AddOrganizationsPageTest"/>
        </classes>
    </test>
</suite>
