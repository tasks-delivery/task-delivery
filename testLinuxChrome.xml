<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test in linux for chrome browser">
    <test name="testLinuxChrome">
        <parameter name="browser" value="chrome"/>
        <parameter name="browserVersion" value="null"/>
        <parameter name="remote" value="http://localhost:4444/wd/hub"/>
        <parameter name="headless" value="false"/>
        <parameter name="timeout" value="8000"/>
        <parameter name="baseUrl" value="http://task-delivery-test-linux-chrom.herokuapp.com/"/>
        <classes>
            <class name="selenium.main.MainPageTest"/>
        </classes>
    </test>
</suite>