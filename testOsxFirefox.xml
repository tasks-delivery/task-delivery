<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test in osx for firefox browser">
    <test name="testOsxFirefox">
        <parameter name="browser" value="firefox"/>
        <parameter name="browserVersion" value="null"/>
        <parameter name="remote" value="http://localhost:4444/wd/hub"/>
        <parameter name="headless" value="false"/>
        <parameter name="timeout" value="8000"/>
        <parameter name="baseUrl" value="https://task-delivery-test-osx-ff.herokuapp.com/"/>
        <classes>
            <class name="selenium.mainPage.MainPageTest"/>
            <class name="selenium.contacts.AddContactsPageTest"/>
            <class name="selenium.organizations.AddOrganizationsPageTest"/>
        </classes>
    </test>
</suite>